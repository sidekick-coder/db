{
    "name": "@sidekick-coder/db",
    "version": "0.3.0",
    "description": "Cli Tool to manipulate data from diferent sources",
    "main": "index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sidekick-coder/db.git"
    },
    "keywords": [
        "data",
        "markdown",
        "json",
        "database"
    ],
    "author": "",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "db": "index.js"
    },
    "exports": {
        "./api": {
            "types": "./dist/core/api/index.d.ts",
            "import": "./dist/core/api/index.js",
            "require": "./dist/core/api/index.cjs"
        },
        "./config": {
            "types": "./dist/core/config/index.d.ts",
            "import": "./dist/core/config/index.js",
            "require": "./dist/core/config/index.cjs"
        },
        "./drive": {
            "types": "./dist/core/drive/index.d.ts",
            "import": "./dist/core/drive/index.js",
            "require": "./dist/core/drive/index.cjs"
        },
        "./providers": {
            "types": "./dist/providers/index.d.ts",
            "import": "./dist/providers/index.js",
            "require": "./dist/providers/index.cjs"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.cjs"
        }
    },
    "scripts": {
        "dev": "tsup --watch src/**/**/*.ts",
        "test": "vitest",
        "lint": "eslint",
        "build": "tsup",
        "prepublishOnly": "tsup --dts"
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.10.5",
        "@types/qs": "^6.9.17",
        "eslint": "^9.18.0",
        "tsup": "^8.3.5",
        "tsx": "^4.19.2",
        "typescript": "5.6",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.3"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.2.3",
        "@poppinss/cliui": "^6.4.2",
        "@sidekick-coder/compositor": "^0.1.3",
        "@sidekick-coder/eslint-config": "^1.0.1",
        "@types/minimist": "^1.2.5",
        "cli-table3": "^0.6.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "fast-glob": "^3.3.3",
        "lodash-es": "^4.17.21",
        "minimist": "^1.2.8",
        "qs": "^6.13.1",
        "sift": "^17.1.3",
        "valibot": "^1.0.0-beta.11",
        "yaml": "^2.7.0"
    }
}
